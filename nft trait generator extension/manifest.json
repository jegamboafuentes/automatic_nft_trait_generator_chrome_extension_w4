{
    "name": "Automatic NFT trait generator",
    "description": "Our extension makes it easier than ever to add unique traits to your NFTs, with just a few clicks.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "48": "assets/48.png",
        "72": "assets/72.png",
        "96": "assets/96.png",
        "144": "assets/144.png"
      },
    "background": {
        "service_worker": "welcome/background.js",
        "type":"module"
    },
    "action": {
        "default_popup": "ext/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "declarativeContent"
    ]
}